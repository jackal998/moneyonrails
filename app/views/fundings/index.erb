            <% colors = ["bg-success", "bg-primary", "bg-secondary", "bg-info", "bg-warning", "bg-danger"] %>
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-3 col-sm-6">
                        <div class="card">
                            <div class="stat-widget-two card-body">
                                <div class="stat-content">
                                    <div class="stat-text">現貨價值</div>
                                    <div class="stat-digit"> <i class="fa fa-usd"></i><%= balances["totalusdValue"].round(2) %></div>
                                </div>
                                <div class="progress">
                                  <% pg_i = 0 %>
                                  <% balances.each do |c, usdValue| %>
                                    <% next if c == "totalusdValue" %>
                                    <% percent = (usdValue / balances["totalusdValue"]) * 100.round(0) %>
                                    <div class="progress-bar <%= colors[pg_i] %>" style="width: <%= percent %>%" role="progressbar" aria-valuenow="<%= percent %>" aria-valuemin="0" aria-valuemax="100">
                                      <%= c %>
                                    </div> 
                                    <% pg_i += 1 %>
                                  <% end %>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-sm-6">
                        <div class="card">
                            <div class="stat-widget-two card-body">
                                <div class="stat-content">
                                  <% leverage = (@ftx_account["result"]["totalPositionSize"]/@ftx_account["result"]["collateral"]).round(2) if @ftx_account["result"]["collateral"] %>
                                  <% percent = leverage * 100.round(0) %>
                                  <div class="stat-text">持倉槓桿</div>
                                  <div class="stat-digit">
                                    <%= leverage %> <i class="fa fa-times"></i>
                                  </div>
                                </div>
                                <div class="progress">
                                  <% lev_color = "bg-success" %>
                                  <% lev_color = "bg-warning" if percent > 160 %>
                                  <% lev_color = "bg-danger" if percent > 300 %>
                                    <div class="progress-bar progress-bar-striped progress-bar-animated <%= lev_color %>" style="width: <%= percent %>%" role="progressbar" aria-valuenow="<%= percent %>" aria-valuemin="0" aria-valuemax="100"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-sm-6">
                        <div class="card">
                            <div class="stat-widget-two card-body">
                                <div class="stat-content">
                                    <div class="stat-text">30 日收益率</div>
                                    <div class="stat-digit"> <i class="fa fa-usd"></i> 500</div>
                                </div>
                                <div class="progress">
                                    <div class="progress-bar bg-warning w-50" role="progressbar" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100"></div>
                                </div>
                            </div>
                        </div>
                    </div>
